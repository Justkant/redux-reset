{
  "name": "redux-reset",
  "version": "1.0.0",
  "description": "Higher Order Reducer for resetting multiple parts of your state tree",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "testonly": "mocha test/index.js",
    "lint": "eslint .",
    "prepublish": "babel ./src --ignore test --out-dir ./dist"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "6.3.21",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "6.2.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.3.3",
    "react-tools": "^0.13.3",
    "redux": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhiaiyer91/redux-reset.git"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  },
  "keywords": [
    "redux",
    "react",
    "Javascript"
  ],
  "author": "Abhi Aiyer",
  "license": "ISC"
}
